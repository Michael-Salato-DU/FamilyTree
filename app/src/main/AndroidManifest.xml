<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="familytree.a188project1.du.bestdamtree">

    <application
        android:name=".FamilyTreeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizActivity" />
        <activity
            android:name=".TreeActivity"
            android:label="@string/title_activity_tree"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PersonActivity"
            android:theme="@style/AppTheme.PersonTheme"/>
        <activity android:name=".NewPersonActivity"
            android:theme="@style/AppTheme.PersonTheme"/>
        <activity android:name=".EditPersonActivity"
            android:theme="@style/AppTheme.PersonTheme"/>
    </application>

</manifest>